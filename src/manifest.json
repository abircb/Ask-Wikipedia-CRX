{
  "manifest_version": 2,
  "name": "Ask Wikipedia",
  "description": "Search Wikipedia with one-click",
  "version": "1.0.2",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["/js/eventPage.js"],
    "persistent": false
  },

  "permissions": [
          "tabs",
          "contextMenus"
        ]
}
